<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <center>
            <table  style="width: 563px">
                &nbsp;
                <p>
                    <p><font size="6"><b>User Creation</b></font></p>
                </p>
                &nbsp;
               <tr>
                   <td>Email Address:</td>
                   <td><g:TextBox ui:field="emailField"/></td>
               </tr>
               <tr>
                   <td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="emailLabel"/></FONT></td>
               </tr>
               <tr>
                   <td>Password:</td>
                   <td><g:TextBox ui:field="passwordField"/>
                   <g:Button ui:field="generatePasswordButton" text="Generate Password"/></td>
               </tr>
               <tr>
                   <td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="passwordLabel"/></FONT></td>
               </tr>
               <tr>
                   <td> Confirm Password: </td>
                   <td><g:TextBox ui:field="confirmPasswordField"/></td>
               </tr>
               <tr>
                   <td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="confirmPasswordLabel"/></FONT></td>
               </tr>
               <tr>
                   <td>First Name:</td>
                   <td><g:TextBox ui:field="firstNameField"/></td>
               </tr>
               <tr>
                   <td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="firstNameLabel"/></FONT></td>
               </tr>
               <tr>
                   <td>Last Name:</td>
                   <td><g:TextBox ui:field="lastNameField"/></td>
               </tr>
               <tr>
                   <td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="lastNameLabel"/></FONT></td>
               </tr>
               <tr>
                   <td>Country:</td>
                   <td>
                       <g:ListBox ui:field="countryField" visibleItemCount="1">
                           <g:item value="1">Singapore</g:item>
                           <g:item value="2">Malaysia</g:item>
                           <g:item value="3">Thailand</g:item>
                           <g:item value="4">China</g:item>
                           <g:item value="5">Hong Kong</g:item>
                           <g:item value="6">Taiwan</g:item>
                       </g:ListBox>
                   </td>
                   <td>
                       <g:ListBox ui:field="regionField" visibleItemCount="1" visible="false">
                           <g:item value="1">Beijing</g:item>
                           <g:item value="2">Shanghai</g:item>
                           <g:item value="3">Wuhan</g:item>
                           <g:item value="4">Dalian</g:item>
                           <g:item value="5">Jinan</g:item>
                           <g:item value="6">Others</g:item>
                       </g:ListBox><br/>
                   </td>
               </tr>
               <tr><td width="174">&nbsp;</td></tr>
               <tr>
                   <td>Contact:</td>
                   <td>
                       <g:ListBox ui:field="countryCodeField" visibleItemCount="1">
                           <g:item value="1">65</g:item>
                           <g:item value="2">60</g:item>
                           <g:item value="3">66</g:item>
                           <g:item value="4">86</g:item>
                           <g:item value="5">852</g:item>
                           <g:item value="6">886</g:item>
                       </g:ListBox>
                       <g:TextBox ui:field="contactField"/>
                   </td>
               </tr>
               <tr><td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="contactLabel"/></FONT></td>
               </tr>
               <tr>
                   <td>School:</td>
                   <td><g:TextBox ui:field="schoolField"/></td>
               </tr>
               <tr>
                   <td width="174">&nbsp;</td>
                   <td><FONT COLOR="red"><g:Label ui:field="schoolLabel"/></FONT></td>
               </tr>
               <tr>
                   <td>Lecturer's First Name: </td>
                   <td><g:TextBox ui:field="lecturerFirstNameField"/></td>
               </tr>
               <tr><td width="174">&nbsp;</td></tr>
               <tr>
                   <td>Lecturer's Last Name: </td>
                   <td><g:TextBox ui:field="lecturerLastNameField"/></td>
               </tr>
               <tr><td width="174">&nbsp;</td></tr>
               <tr>
                   <td>Lecturer's Email: </td>
                   <td><g:TextBox ui:field="lecturerEmailField"/></td>
               </tr>
               <tr><td width="174">&nbsp;</td></tr>
               <tr>
                   <td>Language:</td>
                   <td>
                       <g:ListBox ui:field="languageField" visibleItemCount="1">
                           <g:item value="1">English</g:item>
                           <g:item value="2">Chinese (Simplified)</g:item>
                           <g:item value="3">Chinese (Traditional)</g:item>
                       </g:ListBox>
                   </td>
               </tr>
               <tr><td width="174">&nbsp;</td></tr>
               <tr>
                   <td>Set Verified: </td>
                   <td><g:CheckBox ui:field="verifiedField" /></td>
               </tr>
               <tr><td width="174">&nbsp;</td></tr>
               <tr>
                   <td align="right"> <g:Button ui:field="registerButton" text="Create"/></td>
                   <td><g:Button ui:field="backButton" text="Back"/></td>
               </tr>
               <tr>
                   <td colspan="4"><g:Label ui:field="messageLabel"/></td>
               </tr>
           </table>
       </center>
    </g:HTMLPanel>
</ui:UiBinder>